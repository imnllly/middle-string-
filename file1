bool itc_isDigit(unsigned char c) {
    if(c >= '0' && c <= '9') return 1;
    return 0;
}

unsigned char itc_toUpper(unsigned char c) {
    if(sym >= 'a' && sym <= 'z') {
        k = sym - 32;
    }
    return k;
} // доделать

unsigned char itc_changeCase(unsigned char c) {
    char k;
    if(c >= 'a' && c <= 'z') k = c - 32;
    else if(c >= 'A' && c <= 'Z') k = c + 32;
    else return c;
    return k;
}
